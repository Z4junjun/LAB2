/* Components/diceset/diceset.wxss */
.container1 {
  display: flex;
  justify-content: center;
  align-items: center;
  width: 50px;
  height: 50px;
}
.container1 > dice{
  transform-style: preserve-3d;
}
.d1{
  animation: run1 1.5s linear forwards;
}
.d2{
  animation: run2 1.5s linear forwards;
}
.d3{
  animation: run3 1.5s linear forwards;
}
.d4{
  animation: run4 1.5s linear forwards;
}
.d5{
  animation: run5 1.5s linear forwards;
}
.d6{
  animation: run6 1.5s linear forwards;
}
@keyframes run1 {
  0%{
    transform: rotateX(1440deg) rotateY(1440deg);
  }
  100%{
    transform: rotateX(0deg);
  }
}
@keyframes run2 {
  0%{
    transform: rotateX(1440deg) rotateY(1440deg);
  }
  100%{
    transform: rotateX(-90deg);
  }
}
@keyframes run3 {
  0%{
    transform: rotateY(1440deg) rotateX(1440deg);
  }
  100%{
    transform: rotateY(90deg);
  }
}
@keyframes run4 {
  0%{
    transform: rotateY(1440deg) rotateX(1440deg);
  }
  100%{
    transform: rotateY(-90deg);
  }
}
@keyframes run5 {
  0%{
    transform: rotateX(1440deg) rotateY(1440deg);
  }
  100%{
    transform: rotateX(90deg);
  }
}
@keyframes run6 {
  0%{
    transform: rotateX(1440deg) rotateY(1440deg);
  }
  100%{
    transform: rotateX(180deg);
  }
}